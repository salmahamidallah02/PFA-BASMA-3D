= {idx: label for label, idx in self.label_dict.items()}\n        \n        print(f\"✅ Données chargées: {len(self.test_df)} échantillons\")\n        print(f\"📋 Classes détectées: {self.labels}\")\n        \n    def load_model(self, model_path):\n        \"\"\"Charge le modèle pré-entraîné\"\"\"\n        if os.path.exists(model_path):\n            print(f\"🤖 Chargement du modèle: {model_path}\")\n            self.model = load_model(model_path)\n            print(\"✅ Modèle chargé avec succès!\")\n        else:\n            print(f\"❌ Modèle introuvable: {model_path}\")\n            raise FileNotFoundError(f\"Modèle non trouvé: {model_path}\")\n    \n    def preprocess_sequence(self, sequence_str):\n        \"\"\"Préprocess une séquence pour la prédiction\"\"\"\n        try:\n            # Convertir la chaîne en liste\n            sequence = eval(sequence_str) if isinstance(sequence_str, str) else sequence_str\n            # Padding\n            padded = pad_sequences([sequence], maxlen=self.maxlen, padding='post', truncating='post')\n            return padded\n        except:\n            print(f\"⚠️ Erreur lors du preprocessing de: {sequence_str}\")\n            return None\n    \n    def predict_single(self, sequence_str):\n        \"\"\"Fait une prédiction sur une séquence\"\"\"\n        processed_seq = self.preprocess_sequence(sequence_str)\n        if processed_seq is None:\n            return None, 0.0\n            \n        # Prédiction\n        prediction = self.model.predict(processed_seq, verbose=0)\n        predicted_class_idx = np.argmax(prediction)\n        confidence = np.max(prediction)\n        \n        predicted_label = self.reverse_label_dict[predicted_class_idx]\n        return predicted_label, confidence\n    \n    def test_random_samples_per_class(self, samples_per_class=3):\n        \"\"\"Teste des échantillons aléatoires pour chaque classe\"\"\"\n        print(\"\\n\" + \"=\"*80)\n        print(\"🧪 TEST DES PRÉDICTIONS PAR CLASSE\")\n        print(\"=\"*80)\n        \n        total_tests = 0\n        correct_predictions = 0\n        \n        for class_label in self.labels:\n            print(f\"\\n🏷️  CLASSE: {class_label}\")\n            print(\"-\" * 60)\n            \n            # Récupérer les échantillons de cette classe\n            class_samples = self.test_df[self.test_df['label'] == class_label]\n            \n            if len(class_samples) == 0:\n                print(f\"⚠️ Aucun échantillon trouvé pour la classe {class_label}\")\n                continue\n            \n            # Sélectionner des échantillons aléatoires\n            n_samples = min(samples_per_class, len(class_samples))\n            selected_samples = class_samples.sample(n=n_samples, random_state=42)\n            \n            class_correct = 0\n            \n            for idx, (_, row) in enumerate(selected_samples.iterrows(), 1):\n                sequence = row['sequence_tokenized']\n                true_label = row['label']\n                \n                # Prédiction\n                predicted_label, confidence = self.predict_single(sequence)\n                \n                if predicted_label is None:\n                    print(f\"  {idx}. ❌ ERREUR DE PRÉDICTION\")\n                    continue\n                \n                # Vérifier si la prédiction est correcte\n                is_correct = predicted_label == true_label\n                status_icon = \"✅\" if is_correct else \"❌\"\n                \n                print(f\"  {idx}. {status_icon} Vrai: {true_label} | Prédit: {predicted_label} | Confiance: {confidence:.3f}\")\n                \n                # Enregistrer les résultats\n                self.results.append({\n                    'true_label': true_label,\n                    'predicted_label': predicted_label,\n                    'confidence': confidence,\n                    'is_correct': is_correct,\n                    'sequence_sample': str(sequence)[:100] + '...'  # Échantillon de séquence\n                })\n                \n                total_tests += 1\n                if is_correct:\n                    correct_predictions += 1\n                    class_correct += 1\n            \n            # Score pour cette classe\n            class_accuracy = (class_correct / n_samples) * 100 if n_samples > 0 else 0\n            print(f\"  📊 Score classe {class_label}: {class_correct}/{n_samples} = {class_accuracy:.1f}%\")\n        \n        # Score global\n        overall_accuracy = (correct_predictions / total_tests) * 100 if total_tests > 0 else 0\n        \n        print(\"\\n\" + \"=\"*80)\n        print(\"📊 RÉSULTATS FINAUX\")\n        print(\"=\"*80)\n        print(f\"🎯 Score global: {correct_predictions}/{total_tests} = {overall_accuracy:.2f}%\")\n        print(f\"📈 Prédictions correctes: {correct_predictions}\")\n        print(f\"❌ Prédictions incorrectes: {total_tests - correct_predictions}\")\n        \n        return overall_accuracy, self.results\n    \n    def detailed_analysis(self):\n        \"\"\"Analyse détaillée des résultats par classe\"\"\"\n        if not self.results:\n            print(\"⚠️ Aucun résultat à analyser\")\n            return\n        \n        print(\"\\n\" + \"=\"*80)\n        print(\"📈 ANALYSE DÉTAILLÉE PAR CLASSE\")\n        print(\"=\"*80)\n        \n        # Grouper par classe réelle\n        class_stats = defaultdict(lambda: {'total': 0, 'correct': 0, 'confidences': []})\n        \n        for result in self.results:\n            true_label = result['true_label']\n            class_stats[true_label]['total'] += 1\n            class_stats[true_label]['confidences'].append(result['confidence'])\n            if result['is_correct']:\n                class_stats[true_label]['correct'] += 1\n        \n        # Afficher les statistiques\n        for class_label in sorted(class_stats.keys()):\n            stats = class_stats[class_label]\n            accuracy = (stats['correct'] / stats['total']) * 100\n            avg_confidence = np.mean(stats['confidences'])\n            \n            print(f\"🏷️  {class_label}:\")\n            print(f\"    Précision: {stats['correct']}/{stats['total']} = {accuracy:.1f}%\")\n            print(f\"    Confiance moyenne: {avg_confidence:.3f}\")\n            print()\n    \n    def save_results(self, output_path=\"/kaggle/working/test_results.json\"):\n        \"\"\"Sauvegarde les résultats au format JSON\"\"\"\n        if not self.results:\n            print(\"⚠️ Aucun résultat à sauvegarder\")\n            return\n        \n        # Préparer les données pour la sauvegarde\n        save_data = {\n            'metadata': {\n                'total_tests': len(self.results),\n                'correct_predictions': sum(1 for r in self.results if r['is_correct']),\n                'overall_accuracy': sum(1 for r in self.results if r['is_correct']) / len(self.results) * 100,\n                'classes_tested': list(set(r['true_label'] for r in self.results))\n            },\n            'results': self.results\n        }\n        \n        with open(output_path, 'w', encoding='utf-8') as f:\n            json.dump(save_data, f, indent=2, ensure_ascii=False)\n        \n        print(f\"💾 Résultats sauvegardés: {output_path}\")\n\n# -----------------------------\n# Script principal\n# -----------------------------\ndef main():\n    try:\n        # Initialiser le testeur\n        tester = APIClassTester(\n            model_path=MODEL_PATH,\n            test_data_path=TEST_PATH,\n            token_path=TOKEN_PATH,\n            maxlen=500\n        )\n        \n        # Tester 2-3 échantillons par classe\n        print(\"🚀 Lancement des tests...\")\n        overall_accuracy, results = tester.test_random_samples_per_class(samples_per_class=3)\n        \n        # Analyse détaillée\n        tester.detailed_analysis()\n        \n        # Sauvegarder les résultats\n        tester.save_results()\n        \n        print(f\"\\n🎉 Tests terminés! Score final: {overall_accuracy:.2f}%\")\n        \n    except Exception as e:\n        print(f\"❌ Erreur lors des tests: {e}\")\n        import traceback\n        traceback.print_exc()\n\n# -----------------------------\n# Fonction de test individuel (pour usage manuel)\n# -----------------------------\ndef test_individual_prediction(sequence_tokenized, true_label=\"unknown\"):\n    \"\"\"\n    Fonction pour tester une prédiction individuelle\n    Usage: test_individual_prediction(\"[1, 2, 3, 4, ...]\", \"vraie_classe\")\n    \"\"\"\n    try:\n        tester = APIClassTester(MODEL_PATH, TEST_PATH, TOKEN_PATH)\n        predicted_label, confidence = tester.predict_single(sequence_tokenized)\n        \n        print(f\"🏷️  Vraie classe: {true_label}\")\n        print(f\"🤖 Prédiction: {predicted_label}\")\n        print(f\"📊 Confiance: {confidence:.3f}\")\n        print(f\"✅ Correct: {'Oui' if predicted_label == true_label else 'Non'}\")\n        \n        return predicted_label, confidence\n        \n    except Exception as e:\n        print(f\"❌ Erreur: {e}\")\n        return None, 0.0\n\n# Exécuter le script principal\nif __name__ == \"__main__\":\n    main()","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-09-16T17:26:40.751151Z","iopub.execute_input":"2025-09-16T17:26:40.751451Z","iopub.status.idle":"2025-09-16T17:26:48.008423Z","shell.execute_reply.started":"2025-09-16T17:26:40.751429Z","shell.execute_reply":"2025-09-16T17:26:48.007514Z"}},"outputs":[{"name":"stdout","text":"📁 Chargement des données...\n✅ Données chargées: 2022 échantillons\n📋 Classes détectées: ['Adware', 'Backdoor', 'Downloader', 'Dropper', 'Spyware', 'Trojan', 'Virus', 'Worms', 'benign']\n🤖 Chargement du modèle: /kaggle/working/models/lstm_attention_model.keras\n","output_type":"stream"},{"name":"stderr","text":"/usr/local/lib/python3.11/dist-packages/keras/src/saving/saving_lib.py:757: UserWarning: Skipping variable loading for optimizer 'rmsprop', because it has 21 variable